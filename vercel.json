{
    "version": 2,
    "rewrites": [
        {
            "source": "/(index\\.html)?",
            "has": [
                {
                    "type": "query",
                    "key": "artwork"
                }
            ],
            "destination": "/api/seo"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}