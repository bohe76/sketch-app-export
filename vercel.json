{
    "version": 2,
    "rewrites": [
        {
            "source": "/",
            "has": [
                {
                    "type": "header",
                    "key": "user-agent",
                    "value": ".*(facebookexternalhit|twitterbot|kakaotalk-scrap|kakaobot|slackbot|whatsapp|googlebot|bingbot).*"
                }
            ],
            "destination": "/api/seo"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}