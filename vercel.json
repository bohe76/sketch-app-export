{
    "version": 2,
    "functions": {
        "api/seo.js": {
            "includeFiles": "dist/template.html"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/assets/(.*)",
            "destination": "/assets/$1"
        },
        {
            "source": "/((?!.*\\.).*)",
            "destination": "/api/seo"
        },
        {
            "source": "/",
            "destination": "/api/seo"
        }
    ]
}